<Project Sdk="Microsoft.NET.Sdk">

		<PropertyGroup>
				<TargetFramework>net8.0</TargetFramework>
				<Version>8.0.0-beta.1</Version>
				<Nullable>enable</Nullable>
				<Authors>Alireza Oroumand</Authors>
				<ImplicitUsings>enable</ImplicitUsings>
				<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
				<RepositoryUrl>https://github.com/oroumand/Zamin</RepositoryUrl>
		</PropertyGroup>
		
		<ItemGroup>
				<PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="8.0.0" />				
				<PackageReference Include="Zamin.Core.Domain" Version="7.5.1-beta.1" />
				<PackageReference Include="Zamin.Infra.Data.Sql.Commands" Version="7.5.1-beta.1" />
				<PackageReference Include="Zamin.Extensions.Events.Abstractions" Version="8.0.0-beta.1" />
				<PackageReference Include="Zamin.Extensions.Serializers.Abstractions" Version="8.0.0-beta.1" />
				<PackageReference Include="Zamin.Extensions.UsersManagement.Abstractions" Version="8.0.0-beta.1" />
		</ItemGroup>
		
		<ItemGroup>
				<Using Include="Microsoft.EntityFrameworkCore" />
		</ItemGroup>
</Project>